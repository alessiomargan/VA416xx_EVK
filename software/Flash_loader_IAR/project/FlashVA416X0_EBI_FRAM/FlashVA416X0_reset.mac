
execUserFlashExit()  // Called by debugger on flashloader exit.
{
__var timeout;
__var DEMCR;

  // Set reset vector catch
  DEMCR = __readMemory32(0xE000EDFC, "Memory");
  __writeMemory32(DEMCR | 0x1, 0xE000EDFC, "Memory");
  // Do a System Reset
  __writeMemory32(0x05FA0004, 0xE000ED0C, "Memory");

}
